{
    "builds": [{
        "src": "portfolio_page/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize": "15mb", "runtime": "python3.9"}
    }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "portfolio_page/wsgi.py"
        }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
    
}